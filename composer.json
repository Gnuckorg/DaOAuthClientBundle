{
    "name": "da/oauth-client-bundle",
    "type": "symfony-bundle",
    "description": "DaOAuthClientBundle is a Symfony2's bundle allowing to use an application as an oauth client",
    "keywords": ["oauth", "client", "bundle", "da", "Symfony"],
    "homepage": "https://github.com/Gnuckorg/DaOAuthClientBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection" : ">=2.1",
        "symfony/config" : ">=2.1",
        "symfony/finder" : ">=2.1",
        "hwi/oauth-bundle": "~0.3",
        "da/auth-common-bundle": "dev-master"
    },
    "suggest": {
        "friendsofsymfony/user-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\OAuthClientBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/OAuthClientBundle"
}
